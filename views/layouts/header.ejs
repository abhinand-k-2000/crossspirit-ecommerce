<body>

	<!--wrapper-->
	<div class="wrapper">
		<!--start top header wrapper-->
		<div class="header-wrapper">
			<div class="top-menu">
				<div class="container">
					<nav class="navbar navbar-expand">
						<div class="shiping-title d-none d-sm-flex">Welcome to our clothing store!</div>
						
					
						<ul class="navbar-nav social-link ms-auto">
							<li class="nav-item"><a class="nav-link" href="javascript:;"><i class='bx bxl-facebook'></i></a></li>
							<li class="nav-item"><a class="nav-link" href="javascript:;"><i class='bx bxl-twitter'></i></a></li>
							<li class="nav-item"><a class="nav-link" href="https://www.instagram.com/cross_spirit_fashion/;"><i class='bx bxl-instagram'></i></a></li>
						</ul>
					


					</nav>
				</div>
			</div>
			<div class="header-content" style="background-color: #4DA7C2;">
				<div class="container">
					<div class="row align-items-center gx-4">
						<div class="col-auto">
							<div class="d-flex align-items-center gap-3">
								<div class="mobile-toggle-menu d-inline d-xl-none" data-bs-toggle="offcanvas"
									data-bs-target="#offcanvasNavbar">
									<i class="bx bx-menu"></i>
								</div>
								<div class="logo">
									<!-- <a href="index.html">
										<img src="/userAssets/images/newlogo.png" class="logo-icon" alt="" />
									</a> -->
									<a href="/"><h2 class="fw-bolder fs-4  text-dark">CrossSpirit Fashion</h2>

									</a>
								</div>
							</div>
						</div>
						<div class="col-12 col-xl order-4 order-xl-0">
							<div class="input-group flex-nowrap pb-3 pb-xl-0">

								<div class="container mt-3">
									<form action="/search" method="get">
										<div class="input-group mb-3">
											<input type="text" class="form-control border-dark border border-3" name="searchBar" placeholder="Search for Products">
											<button class="btn btn-dark btn-ecomm border-3" type="submit">Search</button>
										</div>
									</form>
								</div>
								

							</div>
						</div>
						
						<div class="col-auto ms-auto">
							<div class="top-cart-icons">

								<!--Account Wishlist Cart-->

							
									<nav class="navbar navbar-expand">
										<ul class="navbar-nav">
											<li class="nav-item"><a href="/dashboard" class="nav-link cart-link"><i class='bx bx-user'></i></a>
											</li>
											<li class="nav-item" style="position: relative;">
												<a href="/wishlist" class="nav-link cart-link">
													<i class='bx bx-heart'></i>
													<span id="wishlistCount" class="alert-count" style="background-color: black; color: white; border-radius: 50%; padding: 0px 6px; position: absolute; top: 8px; right: -2px; font-size: .5em;">
														<%= wishlistItemCount %>
												</a>
											</li>
											
											<li class="nav-item" style="position: relative;">
												<a href="/cart" class="nav-link cart-link">
													<i class='bx bx-shopping-bag'></i>
													<span class="alert-count fw-bolder" style="background-color: black; color: white; border-radius: 50%; padding: 0px 6px; position: absolute; top: 8px; right: -2px; font-size: .5em;">
    													<%= cartItemCount %>
													</span>
												</a>
											</li>
											
											
											

										</ul>
									</nav>
				

								


							</div>
						</div>
					</div>
					<!--end row-->
				</div>
			</div>

			<div class="primary-menu">
				<nav class="navbar navbar-expand-xl w-100 navbar-dark container mb-0 p-0">
					<div class="offcanvas offcanvas-start" tabindex="-1" id="offcanvasNavbar">
					  <div class="offcanvas-header">
						<div class="offcanvas-logo"><img src="/userAssets/images/logo-icon.png" width="100" alt="">
						</div>
						<button type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
					  </div>
					  <div class="offcanvas-body primary-menu">
						<ul class="navbar-nav justify-content-start flex-grow-1 gap-1">
						  <li class="nav-item">
							<a class="nav-link" href="/">Home</a>
						  </li>
						  <li class="nav-item dropdown">
							<a class="nav-link dropdown-toggle dropdown-toggle-nocaret" href="tv-shows.html"
							  data-bs-toggle="dropdown">
							  Categories
							</a>

							<div class="dropdown-menu dropdown-large-menu">
							  <div class="row">
								<div class="col-12 col-xl-4">
								  <h6 class="large-menu-title">Fashion</h6>
								  <ul class="list-unstyled">

									<% if(category.length > 0){
										category.forEach((ele) =>{ 
											if(ele.isDeleted == false){ %>

												<li><a href="javascript:;"><%= ele.name %></a></li>
											<% } %>
											
										<% }) } %>	
										
								  </ul>		
								</div>
							  </div>
							</div>
						  </li>
						  

						  <li class="nav-item">
							<a class="nav-link" href="#">About</a>
						  </li>
						  <li class="nav-item">
							<a class="nav-link" href="#">Contact</a>
						  </li>
						  <li class="nav-item"> <a class="nav-link" href="/shop-grid">Our Store</a> 
						  </li>



 	
				 <% if(!user){ %>
							<li class="nav-item dropdown">
							  <a class="nav-link dropdown-toggle dropdown-toggle-nocaret" href="javascript:;" data-bs-toggle="dropdown">
								Account</a>
								  <ul class="dropdown-menu">
									  <li><a class="dropdown-item" href="/login">Login</a></li>
									  <li><a class="dropdown-item" href="/register">Register</a></li>
								  </ul>
							  <% } %> 
							
						  </li>
						</ul>
					  </div>
					</div>	
				  </nav>
			</div>
		</div>
		<!--end top header wrapper-->

